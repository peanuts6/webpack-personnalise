{
  "presets": [
    [
      "babel-preset-env",
      {
        "targets": {
          "browsers": ["last 3 versions", "ie >= 8"]
        }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-transform-runtime",{
        "helpers": false,
        "polyfill": false,
        "moduleName": "babel-runtime"
    }]
  ]
}
